{
  "manifest_version": 3,
  "name": "Disable Twitch Homepage Video Autoplay",
  "author": "Thor110",
  "update_url": "https://github.com/Thor110/ChromeExtensions",
  "version": "1.0.1",
  "description": "Disables the Twitch Homepage Video from autoplaying.",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://www.twitch.tv/"],
      "js": ["disable.js"]
    }
  ]
}